#include <cs50.h>
#include <stdio.h>

int main(void)
{
    int h;
    do
    {
        printf("height: ");
        h = GetInt();
    }
    while (h <= -1 || h > 23);
    int i;
    int j;
    int k;
    for(i = 0; i < h; i ++)
    {
        for(j = h - 1; j > i; j --)
        {
            printf(" ");
        }
        printf("#");
        for(k = 0; k < i; k ++)
        {
            printf("#");
        }
        printf("#");
        printf("\n");
    }
    return 0;
}
